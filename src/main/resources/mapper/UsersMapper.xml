<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.github.mapper.UsersMapper">
    <select id="findByDisplayName" resultType="com.github.entity.Users">
        select * from users where display_name = #{displayName} limit 1
    </select>
</mapper>